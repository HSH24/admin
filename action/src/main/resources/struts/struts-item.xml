<!DOCTYPE struts PUBLIC
 "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
 "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="{http://admin}admin$item"
		extends="{http://admin}admin$framework" namespace="/item">

		<action name="list" class="itemAction" method="list">
			<result name="success">/admin/item/list.vm</result>
		</action>
		
		<action name="detail" class="itemAction" method="detail">
			<result name="success">/admin/item/detail.vm</result>
		</action>
				
	</package>
</struts>