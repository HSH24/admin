<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<bean id="userOrgAction" class="com.hsh24.admin.user.action.UserOrgAction" parent="baseAction" scope="request">
		<property name="userOrgService" ref="userOrgService" />
	</bean>
	
	<bean id="userOrgService" class="com.hsh24.admin.user.service.impl.UserOrgServiceImpl">
		<property name="userService" ref="userService" />
	</bean>
	
</beans>